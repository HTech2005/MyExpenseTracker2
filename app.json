{
  "expo": {
    "name": "MyExpenseTracker2",
    "slug": "MyExpenseTracker2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff",
          "defaultChannel": "default"
        }
      ]
    ],
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.harold14.MyExpenseTracker",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.harold14.MyExpenseTracker2"
    },
    "web": {
      "favicon": "./assets/icon.png",
      "name": "MyExpenseTracker2",
      "shortName": "ExpenseTracker",
      "description": "Gérez vos dépenses, budgets et dettes facilement",
      "lang": "fr",
      "backgroundColor": "#ffffff",
      "themeColor": "#7c3aed",
      "display": "standalone",
      "orientation": "portrait",
      "scope": "/",
      "startUrl": "/",
      "preferRelatedApplications": false,
      "config": {
        "firebase": {
          "apiKey": "",
          "authDomain": "",
          "projectId": "",
          "storageBucket": "",
          "messagingSenderId": "",
          "appId": ""
        }
      },
      "meta": {
        "apple-mobile-web-app-capable": "yes",
        "apple-mobile-web-app-status-bar-style": "default",
        "apple-mobile-web-app-title": "ExpenseTracker",
        "mobile-web-app-capable": "yes",
        "msapplication-TileColor": "#7c3aed"
      },
      "build": {
        "babel": {
          "include": [
            "@expo/vector-icons"
          ]
        }
      }
    },
    "extra": {
      "eas": {
        "projectId": "e6e1d427-4eef-47f3-8c47-675765b7c05c"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "owner": "harold14"
  }
}