{
  "name": "myexpensetracker2",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "npx cross-env NODE_OPTIONS=--max-old-space-size=4096 expo start",
    "android": "npx cross-env NODE_OPTIONS=--max-old-space-size=4096 expo start --android",
    "ios": "npx cross-env NODE_OPTIONS=--max-old-space-size=4096 expo start --ios",
    "web": "npx cross-env NODE_OPTIONS=--max-old-space-size=4096 expo start --web",
    "build:web": "npx expo export --platform web && node scripts/copy-pwa-assets.js",
    "clean": "npx cross-env NODE_OPTIONS=--max-old-space-size=4096 expo start -c"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "expo": "~54.0.30",
    "expo-linear-gradient": "~15.0.8",
    "expo-notifications": "~0.32.15",
    "expo-status-bar": "~3.0.9",
    "expo-updates": "~29.0.15",
    "lucide-react-native": "^0.544.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-chart-kit": "^6.12.0",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-web": "^0.21.0"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "typescript": "~5.9.2"
  },
  "private": true
}